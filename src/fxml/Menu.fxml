<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar fx:id="menu" prefHeight="25.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MenuController">
  <menus>
    <Menu mnemonicParsing="false" text="Program">
      <items>
        <MenuItem fx:id="iItemStart" mnemonicParsing="false" onAction="#startAction" text="Start" />
        <MenuItem mnemonicParsing="false" onAction="#chooseProjectAction" text="Choose Project" />
        <MenuItem mnemonicParsing="false" onAction="#chooseBaseAction" text="Choose Base" />
      </items>
    </Menu>
      <Menu mnemonicParsing="false" text="Settings">
        <items>
          <MenuItem mnemonicParsing="false" text="Action 1" />
            <MenuItem mnemonicParsing="false" text="Action 2" />
            <MenuItem mnemonicParsing="false" text="Action 3" />
        </items>
      </Menu>
    <Menu mnemonicParsing="false" text="Help">
      <items>
        <MenuItem mnemonicParsing="false" text="About" />
      </items>
    </Menu>
  </menus>
   <padding>
      <Insets right="1.0" />
   </padding>
</MenuBar>
